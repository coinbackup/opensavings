{
  "name": "open-savings-initiative",
  "version": "1.0.3",
  "description": "Lock coins until a future date.",
  "homepage": "https://github.com/coinbackup/opensavings",
  "author": {
    "name": "Ransom Christofferson"
  },
  "license": "MIT",
  "keywords": [
    "bitcoin",
    "savings",
    "check",
    "cheque",
    "nlocktime",
    "checklocktimeverify"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps && node patch.js",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:prod:web": "npm run postinstall:web && ng build -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.5",
    "@angular/animations": "6.1.4",
    "@angular/cdk": "6.4.6",
    "@angular/cli": "6.1.4",
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/compiler-cli": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/forms": "6.1.4",
    "@angular/http": "6.1.4",
    "@angular/language-service": "6.1.4",
    "@angular/material": "6.4.6",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@angular/router": "6.1.4",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "bchaddrjs": "^0.2.1",
    "bitcore-lib": "^0.16.0",
    "bitcore-lib-cash": "^0.19.0",
    "codelyzer": "4.2.1",
    "core-js": "2.5.6",
    "electron": "11.5.0",
    "electron-builder": "20.28.1",
    "electron-reload": "1.2.2",
    "hammerjs": "^2.0.8",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "jsqr": "^1.1.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "node-sass": "^6.0.0",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "5.3.2",
    "qrcode": "^1.2.0",
    "rxjs": "6.2.2",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.0.6",
    "zone.js": "0.8.26"
  },
  "dependencies": {}
}
